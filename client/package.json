    "acorn": "^6.0.4",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^5.0.0",
    "nodemon": "^1.11.0"
  }
}
