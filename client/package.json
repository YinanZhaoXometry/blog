{
  "name": "nuxt-koa-mongo-blog2",
  "version": "1.0.0",
  "description": "My blog created with nuxt-koa2-mongodb",
  "author": "zhaoyn7",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --exec babel-node",
